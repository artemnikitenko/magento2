<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
    <default>
        <carriers>
            <shipbox>
                <active>1</active>
                <sallowspecific>0</sallowspecific>
                <model>Practice\Dev\Model\Carrier\Shipbox</model>
                <name>Dev Example Shipping</name>
                <price>15.00</price>
                <title>Dev Example</title>
                <type>I</type>
                <specificerrmsg>This shipping method is not available. To use this shipping method, please contact us.</specificerrmsg>
            </shipbox>
        </carriers>
        <payment>
            <paybox>
                <active>1</active>
                <model>Practice\Dev\Model\Paybox</model><order_status>pending</order_status>
                <title>Dev Paybox</title>
                <allowspecific>0</allowspecific>
                <group>offline</group>
            </paybox>
        </payment>
    </default>
</config>